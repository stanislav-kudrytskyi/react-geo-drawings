{
  "name": "react-geo-drawings",
  "version": "0.0.15",
  "description": "Unified interface over geo map api providers for React",
  "author": "Stanislav Kudrytskyi <stanislav.kudrytskyi@gmail.com> (https://github.com/stanislav-kudrytskyi)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stanislav-kudrytskyi/react-geo-drawings.git"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "buildDefault": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "vite",
    "build": "tsc && vite build",
    "storybook": "storybook dev -p 4800",
    "build-storybook": "storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@googlemaps/react-wrapper": "^1.1.34",
    "@mui/material": "^5.11.12",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-mdx-gfm": "^7.6.7",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.7",
    "@storybook/preview-api": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/google.maps": "^3.49.2",
    "@types/jest": "^27.5.2",
    "@types/mapbox-gl": "^2.7.10",
    "@types/node": "^16.18.65",
    "@types/react": "^18.0.13",
    "@types/react-dom": "^18.0.5",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rollup": "^3.9.0",
    "rollup-plugin-dts": "^5.1.0",
    "rollup-plugin-import-css": "^3.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "storybook": "^7.6.7",
    "typescript": "^4.7.3",
    "uuid": "^9.0.0",
    "vite": "^5.0.2",
    "web-vitals": "^2.1.4"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.2.0"
  },
  "optionalDependencies": {
    "mapbox-gl": "^2.13.0"
  }
